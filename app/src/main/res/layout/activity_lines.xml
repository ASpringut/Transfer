        <ImageView
            android:id="@+id/Wtrain"
            android:layout_width="58dp"
            android:layout_height="64dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/Rtrain"
            app:layout_constraintTop_toTopOf="@+id/Qtrain"
            app:srcCompat="@drawable/ic_nycs_bull_trans_w" />

        <ImageView
            android:id="@+id/shuttle"
            android:layout_width="64dp"
            android:layout_height="64dp"
            app:layout_constraintEnd_toStartOf="@+id/Ltrain"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/seventrain"
            app:layout_constraintTop_toTopOf="@+id/Ltrain"
            app:srcCompat="@drawable/ic_nycs_bull_trans_s" />

        <ImageView
            android:id="@+id/Ltrain"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toStartOf="@+id/Gtrain"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/shuttle"
            app:layout_constraintTop_toBottomOf="@+id/fivetrain"
            app:srcCompat="@drawable/ic_nycs_bull_trans_l" />

        <ImageView
            android:id="@+id/Gtrain"
            android:layout_width="64dp"
            android:layout_height="64dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/Ltrain"
            app:layout_constraintTop_toTopOf="@+id/Ltrain"
            app:srcCompat="@drawable/ic_nycs_bull_trans_g" />

        <ImageView
            android:id="@+id/Jtrain"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toStartOf="@+id/Ztrain"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Qtrain"
            app:srcCompat="@drawable/ic_nycs_bull_trans_j" />

        <ImageView
            android:id="@+id/Ztrain"
            android:layout_width="64dp"
            android:layout_height="64dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/Jtrain"
            app:layout_constraintTop_toTopOf="@+id/Jtrain"
            app:srcCompat="@drawable/ic_nycs_bull_trans_z" />

    </android.support.constraint.ConstraintLayout>
</ScrollView>