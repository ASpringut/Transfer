<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    tools:context="com.polarizedraven.transfer.LineSelector"
    tools:layout_editor_absoluteY="81dp">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/onetrain"
            android:layout_width="64dp"
            android:layout_height="64dp"
            app:layout_constraintEnd_toStartOf="@+id/twotrain"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/twotrain"
            app:srcCompat="@drawable/ic_nycs_bull_trans_1" />

        <ImageView
            android:id="@+id/twotrain"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toStartOf="@+id/threetrain"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/onetrain"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_nycs_bull_trans_2" />

        <ImageView
            android:id="@+id/threetrain"
            android:layout_width="64dp"
            android:layout_height="64dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/twotrain"
            app:layout_constraintTop_toTopOf="@+id/twotrain"
            app:srcCompat="@drawable/ic_nycs_bull_trans_3" />

        <ImageView
            android:id="@+id/fourtrain"
            android:layout_width="64dp"
            android:layout_height="64dp"
            app:layout_constraintEnd_toStartOf="@+id/fivetrain"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/fivetrain"
            app:srcCompat="@drawable/ic_nycs_bull_trans_4" />

        <ImageView
            android:id="@+id/fivetrain"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toStartOf="@+id/sixtrain"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/fourtrain"
            app:layout_constraintTop_toBottomOf="@+id/twotrain"
            app:srcCompat="@drawable/ic_nycs_bull_trans_5" />

        <ImageView
            android:id="@+id/sixtrain"
            android:layout_width="64dp"
            android:layout_height="64dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/fivetrain"
            app:layout_constraintTop_toTopOf="@+id/fivetrain"
            app:srcCompat="@drawable/ic_nycs_bull_trans_6" />

        <ImageView
            android:id="@+id/seventrain"
            android:layout_width="64dp"
            android:layout_height="64dp"
            app:layout_constraintEnd_toStartOf="@+id/shuttle"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/Ltrain"
            app:srcCompat="@drawable/ic_nycs_bull_trans_7" />

        <ImageView
            android:id="@+id/Atrain"
            android:layout_width="64dp"
            android:layout_height="64dp"
            app:layout_constraintEnd_toStartOf="@+id/Ctrain"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/Ctrain"
            app:srcCompat="@drawable/ic_nycs_bull_trans_a" />

        <ImageView
            android:id="@+id/Ctrain"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toStartOf="@+id/Etrain"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/Atrain"
            app:layout_constraintTop_toBottomOf="@+id/shuttle"
            app:srcCompat="@drawable/ic_nycs_bull_trans_c" />

        <ImageView
            android:id="@+id/Etrain"
            android:layout_width="64dp"
            android:layout_height="64dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/Ctrain"
            app:layout_constraintTop_toTopOf="@+id/Ctrain"
            app:srcCompat="@drawable/ic_nycs_bull_trans_e" />

        <ImageView
